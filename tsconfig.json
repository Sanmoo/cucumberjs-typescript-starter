{
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "types"
  ],
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "outDir": "./built",
    "noImplicitAny": true,
    "noImplicitReturns": true
  }
}
